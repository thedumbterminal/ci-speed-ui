{
  "name": "ci-speed-ui",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@fontsource/roboto": "^5.0.1",
    "@hookform/resolvers": "^3.1.0",
    "@mui/icons-material": "^5.11.16",
    "@mui/lab": "^5.0.0-alpha.131",
    "@mui/material": "^5.13.2",
    "@mui/x-data-grid": "^5.17.25",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.2.5",
    "@types/react": "^18.2.7",
    "@types/react-dom": "^18.2.4",
    "@visx/visx": "^3.1.2",
    "axios": "^1.4.0",
    "date-fns": "^2.30.0",
    "qs": "^6.11.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.44.2",
    "react-router-dom": "^6.11.2",
    "swr": "^2.1.5",
    "typescript": "^4.9.5",
    "use-local-storage-state": "^18.3.3",
    "web-vitals": "^3.3.1",
    "yup": "^1.2.0"
  },
  "scripts": {
    "lint": "npm run lint:prettier && npm run lint:eslint",
    "lint:prettier": "prettier -c .",
    "lint:eslint": "eslint .",
    "format": "prettier --write .",
    "start": "react-scripts start",
    "build": "BUILD_PATH=./docs react-scripts build",
    "test": "react-scripts test --watchAll=false",
    "coverage": "npm run test -- --coverage",
    "coverage:report": "npm run coverage -- --reporters=default --reporters=jest-junit",
    "test:watch": "react-scripts test",
    "eject": "react-scripts eject",
    "deploy": "git checkout main && git pull origin main && npx update-browserslist-db && REACT_APP_API_BASE=https://ci-speed.herokuapp.com/api PUBLIC_URL=/ci-speed-ui npm run build && git add docs && git commit docs -m 'Deploy' && git push origin main",
    "serve": "npm run build && cd docs && python3 -m http.server",
    "ci": "npm run lint && npm run build && npm run coverage:report"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "eslintIgnore": [
    "coverage/*"
  ],
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "devDependencies": {
    "jest-junit": "^16.0.0",
    "prettier": "^2.8.8",
    "react-scripts": "5.0.1",
    "update-browserslist-db": "^1.0.11"
  }
}
