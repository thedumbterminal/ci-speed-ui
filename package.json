{
  "name": "ci-speed-ui",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@fontsource/roboto": "^4.5.8",
    "@hookform/resolvers": "^2.9.10",
    "@mui/icons-material": "^5.11.0",
    "@mui/lab": "^5.0.0-alpha.114",
    "@mui/material": "^5.11.1",
    "@mui/x-data-grid": "^5.17.17",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.2.5",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@visx/visx": "^2.17.0",
    "axios": "^1.2.2",
    "date-fns": "^2.29.3",
    "qs": "^6.11.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.41.3",
    "react-router-dom": "^6.6.1",
    "swr": "^2.0.0",
    "typescript": "^4.9.4",
    "use-local-storage-state": "^18.1.2",
    "web-vitals": "^3.1.0",
    "yup": "^0.32.11"
  },
  "scripts": {
    "lint": "eslint .",
    "format": "prettier --write 'src/**/*.tsx' 'src/**/*.ts'",
    "start": "react-scripts start",
    "build": "BUILD_PATH=./docs react-scripts build",
    "test": "react-scripts test --watchAll=false",
    "coverage": "npm run test -- --coverage",
    "coverage:report": "npm run coverage -- --reporters=default --reporters=jest-junit",
    "test:watch": "react-scripts test",
    "eject": "react-scripts eject",
    "deploy": "git checkout main && git pull origin main && npx update-browserslist-db && REACT_APP_API_BASE=https://ci-speed.herokuapp.com/api PUBLIC_URL=/ci-speed-ui npm run build && git add docs && git commit docs -m 'Deploy' && git push origin main",
    "serve": "npm run build && cd docs && python3 -m http.server",
    "ci": "npm run lint && npm run coverage:report"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "eslintIgnore": [
    "coverage/*"
  ],
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "devDependencies": {
    "jest-junit": "^15.0.0",
    "prettier": "^2.8.1",
    "react-scripts": "5.0.1",
    "update-browserslist-db": "^1.0.10"
  }
}
