{
  "name": "ci-speed-ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "next dev",
    "start:production": "next start",
    "build": "next build",
    "lint": "next lint",
    "dist": "npm run build && next export -o docs",
    "deploy": "BASE_PATH=/ci-speed-ui npm run dist && git add docs && git commit docs -m 'deploy' && git push origin",
    "serve": "npm run dist && cd docs && python3 -m http.server"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.5",
    "axios": "^0.26.1",
    "bootstrap": "^5.1.3",
    "date-fns": "^2.28.0",
    "next": "12.1.5",
    "react": "18.0.0",
    "react-bootstrap": "^2.2.3",
    "react-dom": "18.0.0"
  },
  "devDependencies": {
    "@types/node": "17.0.24",
    "@types/react": "18.0.5",
    "@types/react-dom": "18.0.0",
    "eslint": "8.13.0",
    "eslint-config-next": "12.1.5",
    "sass": "^1.50.0",
    "typescript": "4.6.3"
  }
}
