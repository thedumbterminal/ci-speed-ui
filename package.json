{
  "name": "ci-speed-ui",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@fontsource/roboto": "^4.5.7",
    "@hookform/resolvers": "^2.9.6",
    "@mui/icons-material": "^5.8.4",
    "@mui/lab": "^5.0.0-alpha.90",
    "@mui/material": "^5.8.7",
    "@mui/x-data-grid": "^5.13.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.2.5",
    "@types/jest": "^28.1.4",
    "@types/node": "^18.0.3",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@visx/visx": "^2.12.1",
    "axios": "^0.27.2",
    "date-fns": "^2.28.0",
    "qs": "^6.11.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.33.1",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "swr": "^1.3.0",
    "typescript": "^4.7.4",
    "web-vitals": "^2.1.4",
    "yup": "^0.32.11"
  },
  "scripts": {
    "lint": "eslint .",
    "format": "prettier --write 'src/**/*.tsx' 'src/**/*.ts'",
    "start": "react-scripts start",
    "build": "BUILD_PATH=./docs react-scripts build",
    "test": "react-scripts test --watchAll=false",
    "coverage": "npm run test -- --coverage",
    "coverage:report": "npm run coverage -- --reporters=default --reporters=jest-junit",
    "test:watch": "react-scripts test",
    "eject": "react-scripts eject",
    "deploy": "REACT_APP_API_BASE=https://ci-speed.herokuapp.com/api PUBLIC_URL=/ci-speed-ui npm run build && git add docs && git commit docs -m 'Deploy' && git push origin",
    "serve": "npm run build && cd docs && python3 -m http.server",
    "ci": "npm run lint && npm run coverage:report"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "eslintIgnore": ["coverage/*"],
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "devDependencies": {
    "jest-junit": "^14.0.0",
    "prettier": "^2.7.1"
  }
}
